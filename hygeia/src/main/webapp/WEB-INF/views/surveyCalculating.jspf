		</c:if> 
		
		<c:if test="${dto.diseaseName == '비만'}"> 
			<c:set var="Stack7" value="${dto.value}" /> 
		</c:if> 
		
		<c:if test="${dto.diseaseName == '만성폐쇄성 폐질환'}"> 
			<c:set var="Stack8" value="${dto.value}" /> 
		</c:if> 
		
		<c:if test="${dto.diseaseName == '골다공증'}"> 
