		</c:if> 
		
		<c:if test="${dto.diseaseName == '급성심근경색'}"> 
			<c:set var="Stack5" value="${dto.value}" /> 
		</c:if> 
		
		<c:if test="${dto.diseaseName == '위염'}"> 
