		</c:if> 
		
		<c:if test="${dto.diseaseName == '고지혈증'}"> 
			<c:set var="Stack11" value="${dto.value}" /> 
		</c:if> 
		
		<c:if test="${dto.diseaseName == '건강함'}"> 
			<c:set var="Stack12" value="${dto.value}" /> 
		</c:if> 

