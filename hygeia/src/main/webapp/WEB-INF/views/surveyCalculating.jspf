	<c:forEach items="${result}" var="dto" varStatus="number">

		<c:if test="${dto.diseaseName == '고혈압'}"> 
			<c:set var="Stack1" value="${dto.value}" />
		</c:if> 
		
		<c:if test="${dto.diseaseName == '뇌졸중'}"> 
			<c:set var="Stack2" value="${dto.value}" /> 
